#include <bits/stdc++.h>

using namespace std;

void printBFS(int **graph,int v,int sv,bool *visited){
    queue<int> bfsQ;
    bfsQ.push(sv);
    visited[sv]=true;
    while(bfsQ.size()!=0){
        int currV=bfsQ.front();
        bfsQ.pop();
        cout<<currV<<" ";
        for(int i=0;i<v;i++){
            if(graph[currV][i] && !visited[i]){
                bfsQ.push(i);
                visited[i]=true;
            }
        }
    }
}

int main() {
    int i;
    int v,e;
    cin>>v>>e;
    
    int **graph=new int*[v];
    for(i=0;i<v;i++){
        graph[i]=new int[v];
        for(int j=0;j<v;j++)
        graph[i][j]=0;
    }
    
    for(i=0;i<e;i++){
        int f,s;cin>>f>>s;
        graph[f][s]=1;
        graph[s][f]=1;
    }
    
    bool *visited=new bool[v];
    for(i=0;i<v;i++) visited[i]=false;
    
    printBFS(graph,v,0,visited);
}
